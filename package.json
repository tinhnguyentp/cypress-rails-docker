{
  "name": "cypress-example-docker-circle",
  "version": "3.0.0",
  "description": "Cypress + Docker + CircleCI = ❤️",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test-junit": "cypress run --reporter junit --reporter-options 'mochaFile=junit-results/my-test-output.xml'",
    "test-multiple": "cypress run --reporter mocha-multi-reporters --reporter-options configFile=config.json",
    "testsuite-1": "cypress run -spec cypress/integration/example1 --reporter junit --reporter-options 'mochaFile=junit-results/my-test-output.xml'",
    "testsuite-2": "cypress run -spec cypress/integration/example2 --reporter junit --reporter-options 'mochaFile=junit-results/my-test-output.xml'"

  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^3.0.1",
    "mocha": "^4.0.1",
    "mocha-junit-reporter": "^1.13.0",
    "mocha-multi-reporters": "^1.1.4"
  }
}
